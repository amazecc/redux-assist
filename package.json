{
    "name": "redux-assist",
    "version": "0.1.0",
    "description": "Integrate redux and redux-saga to automatically manage your status",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "webpack-dev-server --config config/webpack.config.dev.ts",
        "build": "rm -rf lib && tsc -p config/tsconfig.build.json",
        "check": "ts-node -r tsconfig-paths/register script/check.ts",
        "release:patch": "npm version patch && npm publish && git push origin master --follow-tags",
        "release:minor": "npm version minor && npm publish && git push origin master --follow-tags",
        "release:major": "npm version major && npm publish && git push origin master --follow-tags"
    },
    "keywords": [
        "redux",
        "redux-assist",
        "redux-helper"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/amazecc/redux-assist.git"
    },
    "author": "Alex",
    "license": "MIT",
    "sideEffects": false,
    "devDependencies": {
        "@babel/cli": "7.5.0",
        "@babel/core": "7.5.0",
        "@babel/plugin-proposal-class-properties": "7.5.0",
        "@babel/plugin-proposal-decorators": "7.4.4",
        "@babel/plugin-transform-typescript": "7.5.1",
        "@babel/preset-env": "7.5.0",
        "@babel/preset-react": "7.0.0",
        "@types/html-webpack-plugin": "3.2.0",
        "@types/node": "12.0.5",
        "@types/react": "16.8.19",
        "@types/react-dom": "16.8.4",
        "@types/react-redux": "7.0.9",
        "@types/webpack": "4.4.32",
        "@types/webpack-dev-server": "3.1.6",
        "babel-loader": "8.0.6",
        "colors": "1.3.3",
        "css-loader": "2.1.1",
        "html-webpack-plugin": "3.2.0",
        "prettier": "1.17.1",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "ts-node": "8.3.0",
        "tsconfig-paths": "3.8.0",
        "tslint": "5.17.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-react": "4.0.0",
        "typescript": "3.5.2",
        "webpack": "4.33.0",
        "webpack-cli": "3.3.2",
        "webpack-dev-server": "3.7.0"
    },
    "dependencies": {
        "@babel/polyfill": "7.4.4",
        "react-redux": "7.0.3",
        "redux": "4.0.1"
    }
}
